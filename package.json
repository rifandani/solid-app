{
  "name": "solid-template",
  "version": "0.0.0",
  "description": "Solid template with Vite, Vitest, TailwindCSS",
  "scripts": {
    "start": "concurrently \"vite\" \"npx json-server --watch db.json --port 5123\"",
    "start:typi": "concurrently \"vite\" \"npx json-server http://jsonplaceholder.typicode.com/db --port 5123\"",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "license": "MIT",
  "devDependencies": {
    "@solidjs/testing-library": "^0.6.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "jsdom": "^21.1.0",
    "json-server": "^0.17.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.6",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-solid": "^2.5.0",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "@solidjs/router": "^0.7.0",
    "axios": "^1.3.3",
    "clsx": "^1.2.1",
    "solid-js": "^1.6.11",
    "tailwind-merge": "^1.9.1"
  }
}
