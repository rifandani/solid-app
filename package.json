{
  "name": "solid-template",
  "version": "0.0.0",
  "description": "Solid template with Vite, Vitest, TailwindCSS, MSW",
  "license": "MIT",
  "author": "Tri Rizeki Rifandani",
  "packageManager": "pnpm@7.28.0",
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write .",
    "type": "tsc --project tsconfig.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "msw:init": "msw init public/ --save",
    "msw:delete": "rm -rf public/mockServiceWorker.js",
    "start": "vite",
    "build": "pnpm msw:delete && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:preview": "vite preview --outDir html",
    "test:preview:coverage": "vite preview --outDir coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@solidjs/testing-library": "^0.8.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitest/coverage-istanbul": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.1.7",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-solid": "^0.12.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "mq-polyfill": "^1.1.8",
    "msw": "^1.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@felte/solid": "^1.2.10",
    "@felte/validator-zod": "^1.0.16",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@kobalte/core": "^0.9.8",
    "@rifandani/nxact-yutiriti": "^1.2.0",
    "@solid-primitives/connectivity": "^0.3.15",
    "@solid-primitives/event-listener": "^2.2.13",
    "@solid-primitives/storage": "^1.3.11",
    "@solid-primitives/timer": "^1.3.7",
    "@solidjs/router": "^0.8.2",
    "@tanstack/solid-query": "^4.29.19",
    "redaxios": "^0.5.1",
    "solid-js": "^1.7.7",
    "tailwind-merge": "^1.13.2",
    "theme-change": "^2.5.0",
    "zod": "^3.21.4"
  }
}
